    "@types/react-highlight-words": "^0.16.4",
    "@types/react-infinite-scroller": "^1.2.3",
    "@umijs/fabric": "^2.14.0",
    "@umijs/plugin-esbuild": "^1.4.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.31",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
