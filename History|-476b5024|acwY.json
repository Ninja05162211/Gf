{
  "name": "basicframework",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged:js"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.3.13",
    "@ant-design/pro-descriptions": "^2.0.11",
    "@ant-design/pro-form": "^2.2.2",
    "@ant-design/pro-layout": "^7.1.3",
    "@ant-design/pro-table": "^3.0.11",
    "@ant-design/pro-utils": "^2.2.2",
    "amfe-flexible": "^2.2.1",
    "antd": "4.23.3",
    "axios": "^0.27.2",
    "history": "^5.3.0",
    "keycloak-js": "^19.0.3",
    "lgutils": "^1.3.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "stylelint": "^14.13.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imp": "^2.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
